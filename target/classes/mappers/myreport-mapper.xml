    <select id="getmychatList" resultType="_int" parameterType="String">
  SELECT count(*)
  FROM CHATINFO
  WHERE USER_ID = #{user_id} OR USER_ID2 = #{user_id} AND STATUS='Y'
  </select>
  
  <select id="selectmychatList" parameterType="string" resultType="ChatInfo2">
  SELECT *
  FROM CHATINFO
	WHERE USER_ID=#{user_id} OR USER_ID2 = #{user_id} AND STATUS='Y'
   ORDER BY CHATID DESC
  </select> 
  
  <update id="myReportUpdate" parameterType="String">
  UPDATE CHATINFO
  SET STATUS='N'
  WHERE USER_ID=#{user_id}
  </update>
  
  
  
